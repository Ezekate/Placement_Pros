@model Core.VeiwModel.ProfileVeiwModel

<!-- Page Content -->
<div class="col-10 mx-auto mt-3">
    <div class="col-12 mt-3" @*style="background-color: #e2e8f0; "*@>

        <!-- Page Header -->
        <div class="">
            <div class="row">
                <div class="col-sm-12">
                    <h3 class="page-title mt-0">Profile</h3>
                    <ul class="breadcrumb">
                        <li class="breadcrumb-item"><a href="index.html">Dashboard</a></li>
                        <li class="breadcrumb-item active">Profile</li>
                    </ul>
                </div>
            </div>
        </div>

        <div class="card mb-2 border border-primary shadow-lg">
            <div class="card-body ">
                <div class="row">
                    <div class="col-2">
                        <a href="#">
                            @if (Model?.PersonalInfo?.ProfilePicture != null)
                            {
                                <img src="@Model?.PersonalInfo?.ProfilePicture" height="100" width="100" class="rounded-circle" alt="Cinque Terre" />
                            }
                            else
                            {
                                <img src="~/image/avatar1.jpg" height="100" width="100" class="rounded-circle" alt="Cinque Terre" />
                            }
                        </a>
                    </div>
                    <div class="col-3">
                        <h3>@Model?.PersonalInfo?.FirstName @Model?.PersonalInfo?.LastName</h3>
                    </div>
                    <div class="col-7">
                        <div class="row mb-2">
                            <div class="title col-3 fw-bold">Phone No:</div>
                            <h6 class="col-6 p -0">@Model?.PersonalInfo?.PhoneNumber</h6>
                            <div class="text ml-5"><a href=""></a></div>
                        </div>
                        <div class="row mb-2">
                            <div class="title  col-3 fw-bold">Email:</div>
                            <h6 class="col-6 p -0">@Model?.PersonalInfo?.Email</h6>
                            <div class="text ml-5"><a mailto:href=""></a></div>
                        </div>
                        <div class="row mb-2">
                            <div class="title col-3 fw-bold ">Birthday:</div>
                            <h6 class="col-6 p -0"> @Model?.PersonalInfo?.Birthdate.ToString("MMMM dd") </h6>
                            <div class="text ml-5"></div>
                        </div>

                        <div class="row mb-2">
                            <div class="title col-3 fw-bold">Gender:</div>
                            <h6 class="col-6 p -0">@Model?.PersonalInfo.Gender</h6>
                            <div class="text ml-5"></div>
                        </div>
                        <div class="row text-nowrap addr mb-2">
                            <div class="title col-3 fw-bold">Address:</div>
                            <h6 class="col-6 p -0">@Model?.PersonalInfo?.Address</h6>
                            <div class="text addr_text"></div>
                        </div>
                        <div class="pro-edit border border-1 rounded-circle px-2 bg-light ">
                            <a data-target="#PersonalInfo" data-toggle="modal" class="edit-icon" onclick="" href="#"><i class="fa fa-pencil text-dark"></i></a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="card tab-box mb-3">
        <div class="row user-tabs">
            <div class="col-lg-12 col-md-12 col-sm-12 line-tabs">
                <ul class="nav nav-tabs nav-tabs-bottom">
                    <li class="nav-item "><a href="#emp_profile" data-toggle="tab" class="nav-link active">Profile</a></li>
                </ul>
            </div>
        </div>
    </div>



    <div class="row">
        <div class="col-md-6 d-flex mt-2 ">
            <div class="card profile-box flex-fill">
                <div class="card-body border border-primary shadow-lg">
                    <h3 class="card-title">Educational Qualification <a href="#" data-toggle="modal" data-target="#EducationalQualification"><i class="fa fa-plus text-dark bg-light" style="font-size:19px; float:right;"></i></a></h3>
                    <div class="experience-box">
                        @if (Model.EducationalQualifications != null)
                        {
                            <ul class="experience-list">
                                @foreach (var education in Model?.EducationalQualifications)
                                {
                                    <li>
                                        <div class="experience-user">
                                            <div class="before-circle"></div>
                                        </div>
                                        <div class="experience-content">
                                            <a class="" data-toggle="modal" data-target="#EditEducationalQualification" onclick="GetQualificationById('@education.Id')"><i class="fa fa-pencil text-dark bg-light " style="font-size:19px; float:right;"></i> </a>

                                            <div class="timeline-content">
                                                <a href="#/" id="@education.Id" class="name">@education.Name</a>
                                                <div>@education.Degree</div>
                                                <span class="time">@education.StartDate.ToString("Y") - @education.EndDate.ToString("Y")</span>
                                                <div>@education.FieldOfStudy</div>
                                                <div>@education.Grade</div>

                                            </div>
                                        </div>

                                    </li>

                                }
                            </ul>
                        }
                    </div>
                </div>
            </div>
        </div>

        <div class="col-md-6 d-flex mt-2 ">
            <div class="card profile-box flex-fill border border-primary ">
                <div class="card-body ">
                    <h3 class="card-title">Skill <a href="#" data-toggle="modal" data-target="#Skills"><i class="fa fa-plus text-dark bg-light " style="font-size: 19px; float: right;"></i></a></h3>
                    <div class="experience-box">
                        @if (Model?.Skills != null)
                        {
                            <ul class="experience-list">

                                @foreach (var skill in Model.Skills)
                                {
                                    <li>
                                        <div class="experience-user">
                                            <div class="before-circle"></div>
                                        </div>
                                        <div class="experience-content">
                                            <a class="" data-toggle="modal" data-target="#EditSkills" onclick="GetSkillById('@skill.Id')"><i class="fa fa-pencil  text-dark bg-light " style="font-size:19px; float:right;"></i> </a>

                                            <div class="timeline-content">
                                                <input id="@skill.Id" hidden />
                                                <div class="name">@skill.Name</div>

                                            </div>


                                        </div>
                                    </li>

                                }

                            </ul>
                        }

                    </div>
                </div>


            </div>

        </div>


        <div class="col-md-6 d-flex mt-2 ">
            <div class="card profile-box flex-fill">
            <div class="card-body border border-primary shadow-lg">
                <h3 class="card-title">Work Experience <a href="#" data-toggle="modal" data-target="#WorkExperience"><i class="fa fa-plus text-dark bg-light " style="font-size: 19px; float: right;"></i></a></h3>
                <div class="experience-box">
                    @if (Model?.WorkExperiences != null)
                    {
                        <ul class="experience-list">
                            @foreach (var work in Model.WorkExperiences)
                            {
                                <li>
                                    <div class="experience-user">
                                        <div class="before-circle"></div>
                                    </div>
                                    <div class="experience-content">
                                        <a class="text-white ml-auto" data-toggle="modal" data-target="#EditWorkExperience" onclick="GetWorkById('@work.Id')"><i class="fa fa-pencil text-dark bg-light  " style="font-size: 19px; float: right;"></i> </a>
                                        <div class="timeline-content">
                                            <a href="#/" class="name">@work.WorkPlace</a>

                                            <p>@work.Discription</p>
                                            <p>@work.Location</p>
                                            <p class="time">@work.DateStarted.ToString("Y") - @work.DateStarted.ToString("Y")</p>


                                        </div>
                                    </div>

                                </li>

                            }


                        </ul>
                    }
                </div>
            </div>
            </div>
        </div>
    </div>
</div>

