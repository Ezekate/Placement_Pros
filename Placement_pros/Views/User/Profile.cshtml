<div id="PersonalInfo" class="modal custom-modal fade" role="dialog">
    <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title"> </h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="col-10 ml-2 form-body m-auto  bg-white ">

                    <div>
                        <div class="pt-2">
                            <div class="form-items">
                                <h3> PersonalInfo</h3>
                                <p class="text-info">please complete the form below to update your info</p>
                                @*<form asp-controller="User" asp-action="EditPersonalInfo" method="post" class="requires-validation">*@

                                <div class="row">
                                    <div class="col-md-6 mb-1">
                                        <label>FirstName</label>
                                        <input id="firstname" class="form-control" type="text" name="Firstname" placeholder="Frist Name" value="@Model?.PersonalInfo?.FirstName" style="border:none; outline:none; background-color:#f5f5f5;" required>
                                    </div>
                                    <div class="col-md-6 mb-1">
                                        <label>LastName</label>
                                        <input id="lastname" class="form-control" type="text" name="name" placeholder="LastName" value="@Model?.PersonalInfo?.LastName" style="border:none; outline:none; background-color:#f5f5f5;" required>
                                    </div>
                                    <div class="col-md-6 mb-1">
                                        <label>BirthDate</label>
                                        <input id="birthdate" class="form-control" name="text" type="date" placeholder="Birthdate" value="@Model?.PersonalInfo?.Birthdate" style="border:none; outline:none; background-color:#f5f5f5;" required>
                                    </div>
                                    <div class="col-md-6 mb-1">
                                        <label>Upload ProfilePicture</label>
                                        <input id="profilepix" class="form-control" name="text" type="file" onchange="ConvertPictureToBase64(event)" value="@Model?.PersonalInfo?.ProfilePicture" style="border:none; outline:none; background-color:#f5f5f5;" required>
                                    </div>
                                    <div class="col-md-6 mb-1">
                                        <lable>Email</lable>
                                        <input id="email" class="form-control" type="email" name="email" value="@Model?.PersonalInfo?.Email" placeholder="Email Address" style="border:none; outline:none; background-color:#f5f5f5;" required>
                                    </div>
                                    <div class="col-md-6 ">
                                        <label class=" mr-1">Gender: </label>
                                        <select id="gender" asp-items="@(new SelectList(@ViewBag.Gender,"Id", "Name", Model?.PersonalInfo?.GendeId))" class="form-control" style="border:none; outline:none; background-color:#f5f5f5;" required>
                                        </select>
                                    </div>

                                    <div class="col-md-6 mb-1">
                                        <lable>PhoneNumber</lable>
                                        <input id="Phone" class="form-control" type="number" placeholder="Phone no" value="@Model?.PersonalInfo?.PhoneNumber" style="border:none; outline:none; background-color:#f5f5f5;" required>
                                    </div>

                                    <div class="col-6 md- mb-1 justify-content-md-center">
                                        <label>Country</label>
                                        <select id="country" asp-for="PersonalInfo.CountryId" asp-items="@(new SelectList(@ViewBag.Countries, "Id", "Name",Model.PersonalInfo?.CountryId))" class="form-control" style="border:none; outline:none; background-color:#f5f5f5;" required>
                                        </select>
                                    </div>
                                    <div class="col-md-6 ">
                                        <label class=" mr-1">State: </label>
                                        <select id="state" class="form-control">
                                        </select>
                                    </div>
                                    <div class="col-md-6 mb-1 justify-content-md-center">
                                        <label>Address</label>
                                        <input id="address" class="form-control" name="address" placeholder="" value="@Model?.PersonalInfo?.Address" style="border:none; outline:none; background-color:#f5f5f5;" required>
                                        <div class="valid-feedback">Address field is valid!</div>
                                        <div class="invalid-feedback">Address field cannot be blank!</div>

                                    </div>
                                    <div class="col-md-12 mb-1 ">
                                        <div class="form-group">
                                            <label>Describe your self in few words</label>
                                            <textarea row="30" cols="5" class="form-control" name="text" id="describe" value="@Model?.PersonalInfo?.Discription" style="border:none; outline:none; background-color:#f5f5f5;"required></textarea>
                                        </div>
                                    </div>
                                    <hr>
                                    <div class="  form-button mt-2">
                                        <button id="submit" type="submit" onclick="myPersonalinfo()" class="btn btn-success">Submit</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
