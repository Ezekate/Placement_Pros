<div id="WorkExperience" class="modal custom-modal fade" role="dialog">
    <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title"> WorkExperience</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                @*Experience Modal*@
                <div class="container-fluid m-auto">

                    <form asp-controller="User" asp-action="WorkExperience" method="post">
                        <div class="row">
                            <div class="col-8  m-auto">
                                <div class="card  bg-white ">
                                    <div class="card-body border-danger  ">
                                        <div class="row">

                                            <div class="col-md-6 mb-3">
                                                <label asp-for="WorkExperience.WorkPlace">Company Name</label>
                                                <input asp-for="WorkExperience.WorkPlace" class="form-control" type="text" placeholder="Company name" style="border:none; outline:none; background-color:#f5f5f5;" required>

                                            </div>

                                            <div class="col-md-6 mb-3">
                                                <label asp-for="WorkExperience.Location">Company location</label>
                                                <input asp-for="WorkExperience.Location" class="form-control" type="text" placeholder="Company Location" style="border:none; outline:none; background-color:#f5f5f5;" required>

                                            </div>


                                            <div class="col-md-6 mb-3 ">
                                                <div class="form-group">
                                                    <label asp-for="WorkExperience.DateStarted"></label>
                                                    <input asp-for="WorkExperience.DateStarted" type="date" row="30" cols="5" class="form-control" style="border:none; outline:none; background-color:#f5f5f5;" required>
                                                </div>
                                            </div>
                                            <div class="col-md-6 mb-3 ">
                                                <div class="form-group">
                                                    <label asp-for="WorkExperience.DateClosed"></label>
                                                    <input asp-for="WorkExperience.DateClosed" type="date" row="30" cols="5" class="form-control" style="border:none; outline:none; background-color:#f5f5f5;" required>

                                                </div>

                                            </div>
                                            <div class="col-md-12 mb-3 ">
                                                <div class="form-group">
                                                    <label asp-for="WorkExperience.Discription">What was your job like</label>
                                                    <textarea row="30" cols="5" class="form-control" asp-for="WorkExperience.Discription" style="border:none; outline:none; background-color:#f5f5f5;" required></textarea>
                                                </div>
                                            </div>
                                            <div class="  form-button mt-3 m-auto">
                                                <button id="submit" type="submit" class="btn btn-success">Submit</button>
                                            </div>


                                        </div>



                                    </div>

                                </div>
                            </div>
                        </div>

                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

