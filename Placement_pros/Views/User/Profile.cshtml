    <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title"> WorkExperience</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                @*Experience Modal*@
                <div class="container-fluid m-auto">

                        <div class="row">
                            <div class="col-8  m-auto">
                                <div class="card  bg-white ">
                                    <div class="card-body border-danger  ">
                                        <div class="row">

                                            <div class="col-md-6 mb-3">
                                                <label>Company Name</label>
                                                <input id="workId" hidden>
                                                <input class="form-control" id="workName" type="text" placeholder="Company name" style="border:none; outline:none; background-color:#f5f5f5;" required>

                                            </div>

                                            <div class="col-md-6 mb-3">
                                                <label>Company location</label>
                                                <input  class="form-control" id="worLocation" type="text" placeholder="Company Location" style="border:none; outline:none; background-color:#f5f5f5;" required>

                                            </div>


                                            <div class="col-md-6 mb-3 ">
                                                <div class="form-group">
                                                    <label asp-for="WorkExperience.DateStarted"></label>
                                                    <input asp-for="WorkExperience.DateStarted" type="date" row="30" cols="5" id="worDate" class="form-control" style="border:none; outline:none; background-color:#f5f5f5;" required>
                                                </div>
                                            </div>
                                            <div class="col-md-6 mb-3 ">
                                                <div class="form-group">
                                                    <label asp-for="WorkExperience.DateClosed"></label>
                                                    <input asp-for="WorkExperience.DateClosed" type="date" row="30" cols="5" id="worDateclosed" class="form-control" style="border:none; outline:none; background-color:#f5f5f5;" required>

                                                </div>

                                            </div>
                                            <div class="col-md-12 mb-3 ">
                                                <div class="form-group">
                                                    <label asp-for="WorkExperience.Discription">What was your job like</label>
                                                    <textarea row="30" cols="5" class="form-control" id="worDiscription" asp-for="WorkExperience.Discription" style="border:none; outline:none; background-color:#f5f5f5;" required></textarea>
                                                </div>
                                            </div>
                                            <div class="  form-button mt-3 m-auto">
                                                <button id="submit" type="submit" onclick="EditWorK()" class="btn btn-success">Submit</button>
                                            </div>


                                        </div>



                                    </div>

                                </div>
                            </div>
                        </div>

                </div>
            </div>
        </div>
    </div>
</div>